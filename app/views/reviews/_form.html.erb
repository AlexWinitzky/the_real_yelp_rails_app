<%= form_for @review do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :website %>
  <%= f.text_field :website %>

  <%= f.label :comments %>
  <%= f.text_field :comments %>

  <%= f.submit class: 'btn'%>
<% end %>